{
    "name": "react-vertical-timeline-component",
    "description": "Vertical timeline for React.js",
    "author": "Santos Alarc√≥n",
    "user": "SantosAlarcon",
    "version": "3.5.2",
    "scripts": {
        "start": "catalog start docs",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "test:watch": "jest --watch",
        "test:lint": "eslint . --ignore-path .gitignore",
        "test:lint:fix": "eslint --fix . --ignore-path .gitignore",
        "gh-pages": "catalog build docs",
        "gh-pages:deploy": "gh-pages -d docs/build",
        "dist:css": "cleancss -o style.min.css style.css",
        "dist:es6": "del-cli ./dist-es6 && cross-env BABEL_ENV=es6 babel ./src --out-dir ./dist-es6",
        "dist:modules": "del-cli ./dist-modules && cross-env BABEL_ENV=modules babel ./src --out-dir ./dist-modules",
        "preversion": "npm run test && npm run test:lint",
        "prepublishOnly": "npm run dist:es6 && npm run dist:modules && npm run dist:css",
        "postpublish": "npm run gh-pages && npm run gh-pages:deploy",
        "postinstall": "node lib/post_install.js"
    },
    "main": "dist-modules",
    "module": "dist-es6",
    "jsnext:main": "dist-es6",
    "devDependencies": {
        "@babel/cli": "^7.26.4",
        "@babel/core": "^7.26.7",
        "@babel/eslint-parser": "^7.26.5",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-env": "^7.26.7",
        "@babel/preset-react": "^7.26.3",
        "@babel/preset-typescript": "7.26.0",
        "@material-ui/core": "^4.12.4",
        "@material-ui/icons": "^4.11.3",
        "babel-jest": "^29.7.0",
        "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
        "catalog": "^3.6.2",
        "chai": "^5.1.2",
        "clean-css-cli": "^5.6.3",
        "cross-env": "^7.0.3",
        "del-cli": "^6.0.0",
        "eslint": "^9.19.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-prettier": "^5.2.3",
        "eslint-plugin-react": "^7.37.4",
        "gh-pages": "^6.3.0",
        "git-prepush-hook": "^1.0.2",
        "jest": "^29.7.0",
        "jest-css-modules": "^2.1.0",
        "prettier": "^3.4.2",
        "purecss": "^3.0.0",
        "react": "^19.0.0",
        "react-addons-test-utils": "^15.6.2",
        "react-dom": "^19.0.0",
        "react-github-corner": "^2.5.0",
        "replace": "^1.2.2",
        "rimraf": "^6.0.1",
        "sync-exec": "^0.6.2",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/react": "^16.2.0",
        "@types/jest": "^29.5.14",
        "@types/react": "^19.0.8",
        "@types/react-dom": "^19.0.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/SantosAlarcon/react-vertical-timeline.git"
    },
    "bugs": {
        "url": "https://github.com/SantosAlarcon/react-vertical-timeline/issues"
    },
    "jest": {
        "testEnvironment": "jest-environment-jsdom",
        "collectCoverage": true,
        "moduleFileExtensions": [
            "js",
            "jsx",
            "ts",
            "tsx",
            "json"
        ],
        "moduleNameMapper": {
            "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
        },
        "moduleDirectories": [
            "node_modules",
            "packages"
        ]
    },
    "keywords": [
        "react",
        "reactjs",
        "React Vertical Timeline"
    ],
    "license": "MIT",
    "dependencies": {
        "classnames": "^2.5.1",
        "react-intersection-observer": "^9.15.1"
    },
    "browserslist": [
        "last 2 versions",
        "> 1%",
        "IE 11"
    ]
}
